@prefix kddi-labs: <urn:ogf:network:kddi-labs:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:jgnx:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/jgnx.n3" .

kddi-labs:bi-kddi-labs-jgnx a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort kddi-labs:jgnx-kddi-labs,
        kddi-labs:kddi-labs-jgnx .

kddi-labs:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort kddi-labs:ps-in,
        kddi-labs:ps-out .

kddi-labs:ets a nml:Topology,
        owl:NamedIndividual;
    nml:hasBidirectionalPort kddi-labs:bi-kddi-labs-jgnx,
        kddi-labs:bi-ps;
    nml:hasInboundPort kddi-labs:jgnx-kddi-labs,
        kddi-labs:ps-in;
    nml:hasOutboundPort kddi-labs:kddi-labs-jgnx,
        kddi-labs:ps-out;
    nml:locatedAt kddi-labs:location;
    nsi:managedBy kddi-labs:nsa .

kddi-labs:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "35.879"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "139.517"^^<http://www.w3.org/2001/XMLSchema#float> .

kddi-labs:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: G-Lambda/KDDILabs (KDDI Labs) 
            Project manager: Takahiro Miyamoto  
            email: tk-miyamoto@kddilabs.jp 
            mobile:  81-90-1966-5153  
            skype: takahiro.miyamoto  
            Key software engineer: Takahiro Miyamoto  
            email: tk-miyamoto@kddilabs.jp 
            mobile:  81-90-1966-5153  
            skype: takahiro.miyamoto 
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
            http://210.196.65.114:9352/connectionprovider
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing kddi-labs:ets .

kddi-labs:jgnx-kddi-labs a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:jgnx:2012:jgnx-kddi-labs>;
    nmleth:vlans "1780-1783" .

kddi-labs:kddi-labs-jgnx a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:jgnx:2012:kddi-labs-jgnx>;
    nmleth:vlans "1780-1783" .

kddi-labs:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

kddi-labs:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

