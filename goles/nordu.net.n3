@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nordu: <urn:ogf:network:nordu.net:2012:> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:netherlight.net:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/netherlight.net.n3" .

nordu:bi-nordu-netherlight a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort nordu:netherlight-nordu,
        nordu:nordu-netherlight .

nordu:bi-nordu-uvalight a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort nordu:nordu-uvalight,
        nordu:uvalight-nordu .

nordu:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort nordu:ps-in,
        nordu:ps-out .

<urn:ogf:network:uvalight.net:2012:topology> a nml:Topology,
        owl:NamedIndividual ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/uvalight.net.n3" .

nordu:ets a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort nordu:netherlight-nordu,
        nordu:ps-in,
        nordu:uvalight-nordu ;
    nml:hasOutboundPort nordu:nordu-netherlight,
        nordu:nordu-uvalight,
        nordu:ps-out ;
    nml:locatedAt nordu:location ;
    nsi:managedBy nordu:nsa .

nordu:location a nml:Location,
        owl:NamedIndividual ;
    nml:lat "55.637"^^xsd:float ;
    nml:long "12.641"^^xsd:float .

nordu:nsa a nsi:NSA,
        owl:NamedIndividual ;
    nsi:adminContact """
            NSA: OpenNSA (NORDUnet)
            Project Management: Jerry Sobieski
            email: jerry@nordu.net
            mobile:  1-301-346-1849
            skype: jerry.sobieski
            Software Development: Henrik Thostrup-Jensen
            email: htj@nordu.net
            skype: henrik.thostrup.jensen
        """^^xsd:string ;
    nsi:csProviderEndpoint "https://nsi.nordu.net:9443/NSI/services/ConnectionService"^^xsd:string ;
    nsi:manages nordu:ets .

nordu:netherlight-nordu a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:netherlight-nordu> ;
    nmleth:vlans "1780-1783" .

nordu:nordu-netherlight a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:nordu-netherlight> ;
    nmleth:vlans "1780-1783" .

nordu:nordu-uvalight a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:uvalight.net:2012:nordu-uvalight> ;
    nmleth:vlans "1780-1783" .

nordu:ps-in a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

nordu:ps-out a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

nordu:uvalight-nordu a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:uvalight.net:2012:uvalight-nordu> ;
    nmleth:vlans "1780-1783" .

