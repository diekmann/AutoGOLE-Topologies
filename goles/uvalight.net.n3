@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix uvalight: <urn:ogf:network:uvalight.net:2012:> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:netherlight.net:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort <urn:ogf:network:netherlight.net:2012:uvalight-netherlight> ;
    nml:hasOutboundPort <urn:ogf:network:netherlight.net:2012:netherlight-uvalight> ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/netherlight.net.owl" .

<urn:ogf:network:nordu.net:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort <urn:ogf:network:nordu.net:2012:uvalight-nordu> ;
    nml:hasOutboundPort <urn:ogf:network:nordu.net:2012:nordu-uvalight> ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/nordu.net.owl" .

uvalight:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort uvalight:ps-in,
        uvalight:ps-out .

uvalight:bi-uvalight-netherlight a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort uvalight:netherlight-uvalight,
        uvalight:uvalight-netherlight .

uvalight:bi-uvalight-nordu a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort uvalight:nordu-uvalight,
        uvalight:uvalight-nordu .

uvalight:location a nml:Location,
        owl:NamedIndividual ;
    nml:lat "52.357"^^xsd:float ;
    nml:long "4.953"^^xsd:float ;
    nml:name "uvalight" .

uvalight:nsa a nsi:NSA,
        owl:NamedIndividual ;
    nsi:adminContact """
            NSA: OpenNSA   
            Project manager: Jeroen van der Ham   
            email: vdham@uva.nl mobile: use skype   
            skype: just1sand0s  
            Network Engineer: Ralph Koning 
            email: r.koning@uva.nl  
            skype: ralph-uva-sne
            Key software engineer: Jeroen van der Ham
            email: vdham@uva.nl mobile: use skype   
            skype: just1sand0s  
        """^^xsd:string ;
    nsi:csProviderEndpoint """
            http://nsa.uvalight.nl:9080/NSI/services/ConnectionService/        
        """^^xsd:string ;
    nsi:manages uvalight:topology .

uvalight:topology a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort uvalight:netherlight-uvalight,
        uvalight:nordu-uvalight,
        uvalight:ps-in ;
    nml:hasOutboundPort uvalight:ps-out,
        uvalight:uvalight-netherlight,
        uvalight:uvalight-nordu ;
    nml:locatedAt uvalight:location ;
    nsi:managedBy uvalight:nsa .

uvalight:netherlight-uvalight a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:netherlight-uvalight> ;
    nmleth:vlans "1780-1783" .

uvalight:nordu-uvalight a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:nordu.net:2012:nordu-uvalight> ;
    nmleth:vlans "1780-1783" .

uvalight:ps-in a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

uvalight:ps-out a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

uvalight:uvalight-netherlight a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:uvalight-netherlight> ;
    nmleth:vlans "1780-1783" .

uvalight:uvalight-nordu a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:nordu.net:2012:uvalight-nordu> ;
    nmleth:vlans "1780-1783" .

