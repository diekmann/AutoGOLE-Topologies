@prefix max: <urn:ogf:network:max:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:nordu.net:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/nordu.net.n3" .

<urn:ogf:network:startap.net:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/startap.net.n3" .

max:bi-max-nordu a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort max:max-nordu,
        max:nordu-max .

max:bi-max-startap a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort max:max-startap,
        max:startap-max .

max:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort max:ps-in,
        max:ps-out .

max:ets a nml:Topology,
        owl:NamedIndividual;
    nml:hasBidirectionalPort max:bi-max-nordu,
        max:bi-max-startap,
        max:bi-ps;
    nml:hasInboundPort max:nordu-max,
        max:ps-in,
        max:startap-max;
    nml:hasOutboundPort max:max-nordu,
        max:max-startap,
        max:ps-out;
    nml:locatedAt max:location;
    nsi:managedBy max:nsa .

max:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "38.995"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "-76.933"^^<http://www.w3.org/2001/XMLSchema#float> .

max:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: OpenNSA
            Project manager:
            email:
            mobile:
            skype: 

            Network Engineer: Balasubramania Pillai
            email: bpillai@maxgigapop.net
            skype: b_pillai

            Key software engineer:
            email:
            skype:
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
            http://nsa.maxgigapop.net:9080/NSI/services/ConnectionService/
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing max:ets .

max:max-nordu a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:nordu.net:2012:max-nordu>;
    nmleth:vlans "1780-1783" .

max:max-startap a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:startap.net:2012:max-startap>;
    nmleth:vlans "1780-1783" .

max:nordu-max a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:nordu.net:2012:nordu-max>;
    nmleth:vlans "1780-1783" .

max:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

max:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

max:startap-max a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:startap.net:2012:startap-max>;
    nmleth:vlans "1780-1783" .

