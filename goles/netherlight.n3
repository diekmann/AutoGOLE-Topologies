        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:northernlight.net:2012:netherlight-northernlight>;
    nmleth:vlans "1780-1783" .

netherlight:netherlight-pionier a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:pionier.net:2012:netherlight-pionier>;
    nmleth:vlans "1780-1783" .

netherlight:netherlight-starlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:starlight.net:2012:netherlight-starlight>;
    nmleth:vlans "1780-1783" .

netherlight:netherlight-uvalight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:uvalight.net:2012:netherlight-uvalight>;
    nmleth:vlans "1780-1783" .

netherlight:northernlight-netherlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:northernlight.net:2012:northernlight-netherlight>;
    nmleth:vlans "1780-1783" .

netherlight:pionier-netherlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:pionier.net:2012:pionier-netherlight>;
    nmleth:vlans "1780-1783" .

netherlight:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

netherlight:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

netherlight:starlight-netherlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:starlight.net:2012:starlight-netherlight>;
    nmleth:vlans "1780-1783" .

netherlight:uvalight-netherlight a nml:PortGroup,
