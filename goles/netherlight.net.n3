@prefix netherlight: <urn:ogf:network:netherlight.net:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:czechlight:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/czechlight.n3" .

<urn:ogf:network:geant.net:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/geant.net.n3" .

<urn:ogf:network:nordu.net:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/nordu.net.n3" .

<urn:ogf:network:pionier.net.pl:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/pionier.net.pl.n3" .

<urn:ogf:network:startap.net:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/startap.net.n3" .

<urn:ogf:network:uvalight.net:2012:topology> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/uvalight.net.n3" .

netherlight:bi-netherlight-czechlight a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort netherlight:czechlight-netherlight,
        netherlight:netherlight-czechlight .

netherlight:bi-netherlight-geant a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort netherlight:geant-netherlight,
        netherlight:netherlight-geant .

netherlight:bi-netherlight-nordu a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort netherlight:netherlight-nordu,
        netherlight:nordu-netherlight .

netherlight:bi-netherlight-pionier a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort netherlight:netherlight-pionier,
        netherlight:pionier-netherlight .

netherlight:bi-netherlight-startap a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort netherlight:netherlight-startap,
        netherlight:startap-netherlight .

netherlight:bi-netherlight-uvalight a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort netherlight:netherlight-uvalight,
        netherlight:uvalight-netherlight .

netherlight:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort netherlight:ps-in,
        netherlight:ps-out .

netherlight:ets a nml:Topology,
        owl:NamedIndividual;
    nml:hasBidirectionalPort netherlight:bi-netherlight-czechlight,
        netherlight:bi-netherlight-geant,
        netherlight:bi-netherlight-nordu,
        netherlight:bi-netherlight-pionier,
        netherlight:bi-netherlight-startap,
        netherlight:bi-netherlight-uvalight,
        netherlight:bi-ps;
    nml:hasInboundPort netherlight:czechlight-netherlight,
        netherlight:geant-netherlight,
        netherlight:nordu-netherlight,
        netherlight:pionier-netherlight,
        netherlight:ps-in,
        netherlight:startap-netherlight,
        netherlight:uvalight-netherlight;
    nml:hasOutboundPort netherlight:netherlight-czechlight,
        netherlight:netherlight-geant,
        netherlight:netherlight-nordu,
        netherlight:netherlight-pionier,
        netherlight:netherlight-startap,
        netherlight:netherlight-uvalight,
        netherlight:ps-out;
    nml:locatedAt netherlight:location;
    nsi:managedBy netherlight:nsa .

netherlight:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "52.357"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "4.953"^^<http://www.w3.org/2001/XMLSchema#float> .

netherlight:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            OpenDRAC (SURFnet) 
            Proj Manager: John MacAuley 
            email: john.macauley@surfnet.nl 
            phone:  1-613-220-6817 
            skype: john.macauley
            Software Developer: John MacAuley 
            email: john.macauley@surfnet.nl 
            phone:  1-613-220-6817 
            skype: john.macauley
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
            http://dracproxy01.surfnet.nl:8080/nsi-v1/ConnectionServiceProvider
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing netherlight:ets .

