netherlight:ets a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort netherlight:czechlight-netherlight,
        netherlight:geant-netherlight,
        netherlight:nordu-netherlight,
        netherlight:pionier-netherlight,
        netherlight:ps-in,
        netherlight:startap-netherlight,
        netherlight:uvalight-netherlight ;
    nml:hasOutboundPort netherlight:netherlight-czechlight,
        netherlight:netherlight-geant,
        netherlight:netherlight-nordu,
        netherlight:netherlight-pionier,
        netherlight:netherlight-startap,
        netherlight:netherlight-uvalight,
        netherlight:ps-out ;
    nml:locatedAt netherlight:location ;
    nsi:managedBy netherlight:nsa .

netherlight:location a nml:Location,
        owl:NamedIndividual ;
    nml:lat "52.357"^^xsd:float ;
    nml:long "4.953"^^xsd:float ;
    nml:name "netherlight" .

