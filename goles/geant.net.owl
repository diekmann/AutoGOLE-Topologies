      <nml:Location rdf:about="urn:ogf:network:geant.net:2012:location">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <nml:lat rdf:datatype="http://www.w3.org/2001/XMLSchema#float">48.859</nml:lat>
        <nml:long rdf:datatype="http://www.w3.org/2001/XMLSchema#float">2.295</nml:long>
