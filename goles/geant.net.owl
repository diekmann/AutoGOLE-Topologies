            Key software engineer: Michal Balcerkiewicz  
            email: michalb@man.poznan.pl  
            skype= michalb.psnc
        </nsi:adminContact>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <nsi:csProviderEndpoint rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
           http://gnscdemo.geant.net:8080/nsi/ConnectionProvider
        </nsi:csProviderEndpoint>
        <nsi:managing rdf:resource="urn:ogf:network:geant.net:2012:ets"/>
