@prefix aist: <urn:ogf:network:aist.go.jp:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

aist:bi-aist-jgn-x a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort aist:aist-jgn-x,
        aist:jgn-x-aist .

aist:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort aist:ps-in,
        aist:ps-out .

<urn:ogf:network:jgn-x.jp:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/jgn-x.jp.n3" .

aist:ets a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort aist:jgn-x-aist,
        aist:ps-in ;
    nml:hasOutboundPort aist:aist-jgn-x,
        aist:ps-out ;
    nml:locatedAt aist:location ;
    nsi:managedBy aist:nsa .

aist:location a nml:Location,
        owl:NamedIndividual ;
    nml:lat "36.06"^^xsd:float ;
    nml:long "140.133"^^xsd:float .

aist:nsa a nsi:NSA,
        owl:NamedIndividual ;
    nsi:adminContact """
            NSA:  G-Lambda/AIST  (AIST)
            Project manager: Tomohiro Kudoh 
            email: t.kudoh@aist.go.jp 
            mobile:  81-90-8741-3165 
            skype: t.kudoh 4.3.
            Key software engineer: Atsuko Takefusa 
            email: nsi-gl@m.aist.go.jp 
            mobile:  81-29-861-3091 
            skype: taketake320
        """^^xsd:string ;
    nsi:csProviderEndpoint """
            http://163.220.30.174:28080/nsi2/services/ConnectionProvider
        """^^xsd:string ;
    nsi:manages aist:ets .

aist:aist-jgn-x a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:jgn-x.jp:2012:aist-jgn-x> ;
    nmleth:vlans "1780-1783" .

aist:jgn-x-aist a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:jgn-x.jp:2012:jgn-x-aist> ;
    nmleth:vlans "1780-1783" .

aist:ps-in a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

aist:ps-out a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

