@prefix krlight: <urn:ogf:network:krlight.net:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

krlight:gloriad-krlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:gloriad.net:2012:gloriad-krlight>;
    nmleth:vlans "1780-1783" .

krlight:krlight-gloriad a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:gloriad.net:2012:krlight-gloriad>;
    nmleth:vlans "1780-1783" .

krlight:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "36.366"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "127.359"^^<http://www.w3.org/2001/XMLSchema#float> .

