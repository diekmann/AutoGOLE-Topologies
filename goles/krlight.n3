    nsi:managing krlight:topology .

krlight:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

krlight:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

krlight:topology a nml:Topology,
        owl:NamedIndividual;
    nml:hasInboundPort krlight:gloriad-krlight,
        krlight:ps-in;
    nml:hasOutboundPort krlight:krlight-gloriad,
        krlight:ps-out;
    nml:locatedAt krlight:location;
    nsi:managedBy krlight:nsa .

