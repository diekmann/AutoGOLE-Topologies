@prefix kddilabs: <urn:ogf:network:kddilabs.jp:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:jgnx:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/jgnx.n3" .

kddilabs:ets a nml:Topology,
        owl:NamedIndividual;
    nml:hasBidirectionalPort <urn:ogf:network:kddilabs:2012:bi-kddilabs-jgnx>,
        <urn:ogf:network:kddilabs:2012:bi-ps>;
    nml:hasInboundPort kddilabs:jgnx-kddilabs,
        kddilabs:ps-in;
    nml:hasOutboundPort kddilabs:kddilabs-jgnx,
        kddilabs:ps-out;
    nml:locatedAt kddilabs:location;
    nsi:managedBy kddilabs:nsa .

kddilabs:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "35.879"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "139.517"^^<http://www.w3.org/2001/XMLSchema#float> .

kddilabs:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: G-Lambda/KDDILabs (KDDI Labs) 
            Project manager: Takahiro Miyamoto  
            email: tk-miyamoto@kddilabs.jp 
            mobile:  81-90-1966-5153  
            skype: takahiro.miyamoto  
            Key software engineer: Takahiro Miyamoto  
            email: tk-miyamoto@kddilabs.jp 
            mobile:  81-90-1966-5153  
            skype: takahiro.miyamoto 
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
            http://210.196.65.114:9352/connectionprovider
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing kddilabs:ets .

<urn:ogf:network:kddilabs:2012:bi-kddilabs-jgnx> a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort kddilabs:jgnx-kddilabs,
        kddilabs:kddilabs-jgnx .

<urn:ogf:network:kddilabs:2012:bi-ps> a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort kddilabs:ps-in,
        kddilabs:ps-out .

kddilabs:jgnx-kddilabs a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:jgnx:2012:jgnx-kddilabs>;
    nmleth:vlans "1780-1783" .

kddilabs:kddilabs-jgnx a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:jgnx:2012:kddilabs-jgnx>;
    nmleth:vlans "1780-1783" .

kddilabs:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

kddilabs:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

