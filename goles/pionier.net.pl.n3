@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pionier: <urn:ogf:network:pionier.net.pl:2012:> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:exp.pionier.net.pl:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort <urn:ogf:network:exp.pionier.net.pl:2012:pionier-exp> ;
    nml:hasOutboundPort <urn:ogf:network:exp.pionier.net.pl:2012:exp-pionier> ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/exp.pionier.net.pl.owl" .

<urn:ogf:network:geant.net:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort <urn:ogf:network:geant.net:2012:pionier-geant> ;
    nml:hasOutboundPort <urn:ogf:network:geant.net:2012:geant-pionier> ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/geant.net.owl" .

<urn:ogf:network:netherlight.net:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort <urn:ogf:network:netherlight.net:2012:pionier-netherlight> ;
    nml:hasOutboundPort <urn:ogf:network:netherlight.net:2012:netherlight-pionier> ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/netherlight.net.owl" .

pionier:bi-pionier-exp a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort pionier:exp-pionier,
        pionier:pionier-exp .

pionier:bi-pionier-geant a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort pionier:geant-pionier,
        pionier:pionier-geant .

pionier:bi-pionier-netherlight a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort pionier:netherlight-pionier,
        pionier:pionier-netherlight .

pionier:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort pionier:ps-in,
        pionier:ps-out .

pionier:ets a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort pionier:exp-pionier,
        pionier:geant-pionier,
        pionier:netherlight-pionier,
        pionier:ps-in ;
    nml:hasOutboundPort pionier:pionier-exp,
        pionier:pionier-geant,
        pionier:pionier-netherlight,
        pionier:ps-out ;
    nml:locatedAt pionier:location ;
    nsi:managedBy pionier:nsa .

pionier:location a nml:Location,
        owl:NamedIndividual ;
    nml:lat "52.412"^^xsd:float ;
    nml:long "16.916"^^xsd:float ;
    nml:name "pionier" .

pionier:nsa a nsi:NSA,
        owl:NamedIndividual ;
    nsi:adminContact """
            NSA: AutoBAHN  (GEANT) 
            Project manager: Radek Krzywania  
            email: radek.krzywania@man.poznan.pl  
            skype: Radoslaw.krzywania 
            Software development: Michal Balcerkiewicz  
            email: michalb@man.poznan.pl  
            skype= michalb.psnc
        """^^xsd:string ;
    nsi:csProviderEndpoint """
            http://194.132.53.174:9280/nsi/ConnectionProvider
        """^^xsd:string ;
    nsi:manages pionier:ets .

pionier:exp-pionier a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:exp.pionier.net.pl:2012:exp-pionier> ;
    nmleth:vlans "1780-1783" .

pionier:geant-pionier a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:geant.net:2012:geant-pionier> ;
    nmleth:vlans "1780-1783" .

pionier:netherlight-pionier a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:netherlight-pionier> ;
    nmleth:vlans "1780-1783" .

pionier:pionier-exp a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:exp.pionier.net.pl:2012:pionier-exp> ;
    nmleth:vlans "1780-1783" .

pionier:pionier-geant a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:geant.net:2012:pionier-geant> ;
    nmleth:vlans "1780-1783" .

pionier:pionier-netherlight a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:pionier-netherlight> ;
    nmleth:vlans "1780-1783" .

pionier:ps-in a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

pionier:ps-out a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

