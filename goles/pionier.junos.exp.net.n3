@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pionier: <urn:ogf:network:pionier.junos.exp.net:2012:> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:pionier.net:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/pionier.net.n3" .

pionier:bi-pionier-pionier a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort pionier:pionier-pionier .

pionier:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort pionier:ps-in,
        pionier:ps-out .

pionier:ets a nml:Topology,
        owl:NamedIndividual;
    nml:hasBidirectionalPort pionier:bi-pionier-pionier,
        pionier:bi-ps;
    nml:hasInboundPort pionier:pionier-pionier,
        pionier:ps-in;
    nml:hasOutboundPort pionier:pionier-pionier,
        pionier:ps-out;
    nml:locatedAt pionier:location;
    nsi:managedBy pionier:nsa .

pionier:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "52.412"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "16.916"^^<http://www.w3.org/2001/XMLSchema#float> .

pionier:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: PSNC 
            Project manager: Radek Krzywania  
            email: radek.krzywania@man.poznan.pl  
            skype: Radoslaw.krzywania 
            Software development: Michal Balcerkiewicz  
            email: michalb@man.poznan.pl  
            skype= michalb.psnc
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
             http://194.132.53.174:9282/nsi/ConnectionProvider 
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing pionier:ets .

pionier:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

pionier:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

pionier:pionier-pionier a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:pionier.net:2012:pionier-pionier>;
    nmleth:vlans "1780-1783" .

