@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pionier: <urn:ogf:network:pionier.net:2012:> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:geant:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/geant.n3" .

<urn:ogf:network:netherlight:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/netherlight.n3" .

<urn:ogf:network:nordu:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/nordu.n3" .

pionier:ets a nml:Topology,
        owl:NamedIndividual;
    nml:hasBidirectionalPort <urn:ogf:network:pionier:2012:bi-pionier-geant>,
        <urn:ogf:network:pionier:2012:bi-pionier-netherlight>,
        <urn:ogf:network:pionier:2012:bi-pionier-nordu>,
        <urn:ogf:network:pionier:2012:bi-ps>;
    nml:hasInboundPort pionier:geant-pionier,
        pionier:netherlight-pionier,
        pionier:nordu-pionier,
        pionier:ps-in;
    nml:hasOutboundPort pionier:pionier-geant,
        pionier:pionier-netherlight,
        pionier:pionier-nordu,
        pionier:ps-out;
    nml:locatedAt pionier:location;
    nsi:managedBy pionier:nsa .

pionier:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "52.412"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "16.916"^^<http://www.w3.org/2001/XMLSchema#float> .

pionier:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: AutoBAHN  (GEANT) 
            Project manager: Radek Krzywania  
            email: radek.krzywania@man.poznan.pl  
            skype: Radoslaw.krzywania 
            Software development: Michal Balcerkiewicz  
            email: michalb@man.poznan.pl  
            skype= michalb.psnc
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
            http://194.132.53.174:9280/nsi/ConnectionProvider
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing pionier:ets .

<urn:ogf:network:pionier:2012:bi-pionier-geant> a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort pionier:geant-pionier,
        pionier:pionier-geant .

<urn:ogf:network:pionier:2012:bi-pionier-netherlight> a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort pionier:netherlight-pionier,
        pionier:pionier-netherlight .

<urn:ogf:network:pionier:2012:bi-pionier-nordu> a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort pionier:nordu-pionier,
        pionier:pionier-nordu .

<urn:ogf:network:pionier:2012:bi-ps> a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort pionier:ps-in,
        pionier:ps-out .

pionier:geant-pionier a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:geant:2012:geant-pionier>;
    nmleth:vlans "1780-1783" .

pionier:netherlight-pionier a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:netherlight:2012:netherlight-pionier>;
    nmleth:vlans "1780-1783" .

pionier:nordu-pionier a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:nordu:2012:nordu-pionier>;
    nmleth:vlans "1780-1783" .

pionier:pionier-geant a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:geant:2012:pionier-geant>;
    nmleth:vlans "1780-1783" .

pionier:pionier-netherlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:netherlight:2012:pionier-netherlight>;
    nmleth:vlans "1780-1783" .

pionier:pionier-nordu a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:nordu:2012:pionier-nordu>;
    nmleth:vlans "1780-1783" .

pionier:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

pionier:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

