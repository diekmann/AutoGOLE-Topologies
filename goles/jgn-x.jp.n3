@prefix jgn-x: <urn:ogf:network:jgn-x.jp:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:aist.go.jp:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort <urn:ogf:network:aist.go.jp:2012:jgn-x-aist> ;
    nml:hasOutboundPort <urn:ogf:network:aist.go.jp:2012:aist-jgn-x> ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/aist.go.jp.owl" .

jgn-x:bi-jgn-x-aist a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort jgn-x:aist-jgn-x,
        jgn-x:jgn-x-aist .

jgn-x:bi-jgn-x-kddilabs a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort jgn-x:jgn-x-kddilabs,
        jgn-x:kddilabs-jgn-x .

jgn-x:bi-jgn-x-startap a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort jgn-x:jgn-x-startap,
        jgn-x:startap-jgn-x .

jgn-x:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort jgn-x:ps-in,
        jgn-x:ps-out .

<urn:ogf:network:kddilabs.jp:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort <urn:ogf:network:kddilabs.jp:2012:jgn-x-kddilabs> ;
    nml:hasOutboundPort <urn:ogf:network:kddilabs.jp:2012:kddilabs-jgn-x> ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/kddilabs.jp.owl" .

<urn:ogf:network:startap.net:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort <urn:ogf:network:startap.net:2012:jgn-x-startap> ;
    nml:hasOutboundPort <urn:ogf:network:startap.net:2012:startap-jgn-x> ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/startap.net.owl" .

jgn-x:ets a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort jgn-x:aist-jgn-x,
        jgn-x:kddilabs-jgn-x,
        jgn-x:ps-in,
        jgn-x:startap-jgn-x ;
    nml:hasOutboundPort jgn-x:jgn-x-aist,
        jgn-x:jgn-x-kddilabs,
        jgn-x:jgn-x-startap,
        jgn-x:ps-out ;
    nml:locatedAt jgn-x:location ;
    nsi:managedBy jgn-x:nsa .

jgn-x:location a nml:Location,
        owl:NamedIndividual ;
    nml:lat "35.688"^^xsd:float ;
    nml:long "139.764"^^xsd:float .

jgn-x:nsa a nsi:NSA,
        owl:NamedIndividual ;
    nsi:adminContact """
            NSA: G-Lambda/KDDILabs (KDDI Labs) 
            Project manager name: Jin Tanaka  
            email: tanaka@ote.kddi.com  
            mobile: +81-80-5062-9352   
            skype: jintanapan 
            Key software engineer: Takahiro Miyamoto  
            email: tk-miyamoto@kddilabs.jp 
            mobile:  81-90-1966-5153  
            skype: takahiro.miyamoto 
        """^^xsd:string ;
    nsi:csProviderEndpoint """
            http://202.180.38.130:8080/2012/03/connectionprovider
        """^^xsd:string ;
    nsi:manages jgn-x:ets .

jgn-x:aist-jgn-x a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:aist.go.jp:2012:aist-jgn-x> ;
    nmleth:vlans "1780-1783" .

jgn-x:jgn-x-aist a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:aist.go.jp:2012:jgn-x-aist> ;
    nmleth:vlans "1780-1783" .

jgn-x:jgn-x-kddilabs a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:kddilabs.jp:2012:jgn-x-kddilabs> ;
    nmleth:vlans "1780-1783" .

jgn-x:jgn-x-startap a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:startap.net:2012:jgn-x-startap> ;
    nmleth:vlans "1780-1783" .

jgn-x:kddilabs-jgn-x a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:kddilabs.jp:2012:kddilabs-jgn-x> ;
    nmleth:vlans "1780-1783" .

jgn-x:ps-in a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

jgn-x:ps-out a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

jgn-x:startap-jgn-x a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:startap.net:2012:startap-jgn-x> ;
    nmleth:vlans "1780-1783" .

