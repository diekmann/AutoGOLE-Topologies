@prefix jgnx: <urn:ogf:network:jgnx:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:aist:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/aist.n3" .

<urn:ogf:network:kddi-labs:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/kddi-labs.n3" .

<urn:ogf:network:startap:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/startap.n3" .

jgnx:bi-jgnx-aist a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort jgnx:aist-jgnx,
        jgnx:jgnx-aist .

jgnx:bi-jgnx-kddi-labs a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort jgnx:jgnx-kddi-labs,
        jgnx:kddi-labs-jgnx .

jgnx:bi-jgnx-startap a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort jgnx:jgnx-startap,
        jgnx:startap-jgnx .

jgnx:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort jgnx:ps-in,
        jgnx:ps-out .

jgnx:ets a nml:Topology,
        owl:NamedIndividual;
    nml:hasBidirectionalPort jgnx:bi-jgnx-aist,
        jgnx:bi-jgnx-kddi-labs,
        jgnx:bi-jgnx-startap,
        jgnx:bi-ps;
    nml:hasInboundPort jgnx:aist-jgnx,
        jgnx:kddi-labs-jgnx,
        jgnx:ps-in,
        jgnx:startap-jgnx;
    nml:hasOutboundPort jgnx:jgnx-aist,
        jgnx:jgnx-kddi-labs,
        jgnx:jgnx-startap,
        jgnx:ps-out;
    nml:locatedAt jgnx:location;
    nsi:managedBy jgnx:nsa .

jgnx:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "35.688"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "139.764"^^<http://www.w3.org/2001/XMLSchema#float> .

jgnx:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: G-Lambda/KDDILabs (KDDI Labs) 
            Project manager name: Jin Tanaka  
            email: tanaka@ote.kddi.com  
            mobile: +81-80-5062-9352   
            skype: jintanapan 
            Key software engineer: Takahiro Miyamoto  
            email: tk-miyamoto@kddilabs.jp 
            mobile:  81-90-1966-5153  
            skype: takahiro.miyamoto 
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
            http://202.180.38.130:8080/connectionprovider
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing jgnx:ets .

jgnx:aist-jgnx a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:aist:2012:aist-jgnx>;
    nmleth:vlans "1780-1783" .

jgnx:jgnx-aist a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:aist:2012:jgnx-aist>;
    nmleth:vlans "1780-1783" .

jgnx:jgnx-kddi-labs a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:kddi-labs:2012:jgnx-kddi-labs>;
    nmleth:vlans "1780-1783" .

jgnx:jgnx-startap a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:startap:2012:jgnx-startap>;
    nmleth:vlans "1780-1783" .

jgnx:kddi-labs-jgnx a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:kddi-labs:2012:kddi-labs-jgnx>;
    nmleth:vlans "1780-1783" .

jgnx:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

jgnx:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

jgnx:startap-jgnx a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:startap:2012:startap-jgnx>;
    nmleth:vlans "1780-1783" .

