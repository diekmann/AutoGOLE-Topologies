@prefix jgn: <urn:ogf:network:jgn.nict.go.jp:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:aist.go.jp:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/aist.go.jp.n3" .

<urn:ogf:network:kddilabs.jp:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/kddilabs.jp.n3" .

<urn:ogf:network:startap.net:2012:ets> a nml:Topology,
        owl:NamedIndividual;
    nsi:isReference "https://github.com/jeroenh/AutoGOLE-Topologies/blob/nsiv2/goles/startap.net.n3" .

jgn:bi-jgn-aist a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort jgn:aist-jgn,
        jgn:jgn-aist .

jgn:bi-jgn-kddilabs a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort jgn:jgn-kddilabs,
        jgn:kddilabs-jgn .

jgn:bi-jgn-startap a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort jgn:jgn-startap,
        jgn:startap-jgn .

jgn:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual;
    nml:hasPort jgn:ps-in,
        jgn:ps-out .

jgn:ets a nml:Topology,
        owl:NamedIndividual;
    nml:hasBidirectionalPort jgn:bi-jgn-aist,
        jgn:bi-jgn-kddilabs,
        jgn:bi-jgn-startap,
        jgn:bi-ps;
    nml:hasInboundPort jgn:aist-jgn,
        jgn:kddilabs-jgn,
        jgn:ps-in,
        jgn:startap-jgn;
    nml:hasOutboundPort jgn:jgn-aist,
        jgn:jgn-kddilabs,
        jgn:jgn-startap,
        jgn:ps-out;
    nml:locatedAt jgn:location;
    nsi:managedBy jgn:nsa .

jgn:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "35.688"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "139.764"^^<http://www.w3.org/2001/XMLSchema#float> .

jgn:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: G-Lambda/KDDILabs (KDDI Labs) 
            Project manager name: Jin Tanaka  
            email: tanaka@ote.kddi.com  
            mobile: +81-80-5062-9352   
            skype: jintanapan 
            Key software engineer: Takahiro Miyamoto  
            email: tk-miyamoto@kddilabs.jp 
            mobile:  81-90-1966-5153  
            skype: takahiro.miyamoto 
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint """
            http://202.180.38.130:8080/connectionprovider
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing jgn:ets .

jgn:aist-jgn a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:aist.go.jp:2012:aist-jgn>;
    nmleth:vlans "1780-1783" .

jgn:jgn-aist a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:aist.go.jp:2012:jgn-aist>;
    nmleth:vlans "1780-1783" .

jgn:jgn-kddilabs a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:kddilabs.jp:2012:jgn-kddilabs>;
    nmleth:vlans "1780-1783" .

jgn:jgn-startap a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:startap.net:2012:jgn-startap>;
    nmleth:vlans "1780-1783" .

jgn:kddilabs-jgn a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:kddilabs.jp:2012:kddilabs-jgn>;
    nmleth:vlans "1780-1783" .

jgn:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

jgn:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

jgn:startap-jgn a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:startap.net:2012:startap-jgn>;
    nmleth:vlans "1780-1783" .

