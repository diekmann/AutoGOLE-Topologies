@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix northernlight: <urn:ogf:network:northernlight.net:2012:> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

northernlight:location a nml:Location,
        owl:NamedIndividual;
    nml:lat "55.637"^^<http://www.w3.org/2001/XMLSchema#float>;
    nml:long "12.641"^^<http://www.w3.org/2001/XMLSchema#float> .

northernlight:max-northernlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:max.net:2012:max-northernlight>;
    nmleth:vlans "1780-1783" .

northernlight:netherlight-northernlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:netherlight-northernlight>;
    nmleth:vlans "1780-1783" .

northernlight:northernlight-max a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:max.net:2012:northernlight-max>;
    nmleth:vlans "1780-1783" .

northernlight:northernlight-netherlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:northernlight-netherlight>;
    nmleth:vlans "1780-1783" .

northernlight:northernlight-pionier a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:pionier.net:2012:northernlight-pionier>;
    nmleth:vlans "1780-1783" .

northernlight:northernlight-uvalight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:uvalight.net:2012:northernlight-uvalight>;
    nmleth:vlans "1780-1783" .

northernlight:nsa a nsi:NSA,
        owl:NamedIndividual;
    nsi:adminContact """
            NSA: OpenNSA (NORDUnet)
            Project Management: Jerry Sobieski
            email: jerry@nordu.net
            mobile:  1-301-346-1849
            skype: jerry.sobieski
            Software Development: Henrik Thostrup-Jensen
            email: htj@nordu.net
            skype: henrik.thostrup.jensen
        """^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:csProviderEndpoint "https://orval.grid.aau.dk:9443/NSI/services/ConnectionService"^^<http://www.w3.org/2001/XMLSchema#string>;
    nsi:managing northernlight:topology .

northernlight:onsala-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

northernlight:onsala-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

northernlight:pionier-northernlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:pionier.net:2012:pionier-northernlight>;
    nmleth:vlans "1780-1783" .

northernlight:ps-in a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

northernlight:ps-out a nml:PortGroup,
        owl:NamedIndividual;
    nmleth:vlans "1780-1783" .

northernlight:topology a nml:Topology,
        owl:NamedIndividual;
    nml:hasInboundPort northernlight:max-northernlight,
        northernlight:netherlight-northernlight,
        northernlight:onsala-in,
        northernlight:pionier-northernlight,
        northernlight:ps-in,
        northernlight:uvalight-northernlight;
    nml:hasOutboundPort northernlight:northernlight-max,
        northernlight:northernlight-netherlight,
        northernlight:northernlight-pionier,
        northernlight:northernlight-uvalight,
        northernlight:onsala-out,
        northernlight:ps-out;
    nml:locatedAt northernlight:location;
    nsi:managedBy northernlight:nsa .

northernlight:uvalight-northernlight a nml:PortGroup,
        owl:NamedIndividual;
    nml:isAlias <urn:ogf:network:uvalight.net:2012:uvalight-northernlight>;
    nmleth:vlans "1780-1783" .

