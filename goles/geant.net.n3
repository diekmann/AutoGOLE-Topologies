@prefix geant: <urn:ogf:network:geant.net:2012:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

geant:bi-geant-netherlight a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort geant:geant-netherlight,
        geant:netherlight-geant .

geant:bi-geant-pionier a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort geant:geant-pionier,
        geant:pionier-geant .

geant:bi-ps a nml:BidirectionalPort,
        owl:NamedIndividual ;
    nml:hasPort geant:ps-in,
        geant:ps-out .

<urn:ogf:network:netherlight.net:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/netherlight.net.n3" .

<urn:ogf:network:pionier.net.pl:2012:ets> a nml:Topology,
        owl:NamedIndividual ;
    nsi:isReference "https://raw.github.com/jeroenh/AutoGOLE-Topologies/nsiv2/goles/pionier.net.pl.n3" .

geant:ets a nml:Topology,
        owl:NamedIndividual ;
    nml:hasInboundPort geant:netherlight-geant,
        geant:pionier-geant,
        geant:ps-in ;
    nml:hasOutboundPort geant:geant-netherlight,
        geant:geant-pionier,
        geant:ps-out ;
    nml:locatedAt geant:location ;
    nsi:managedBy geant:nsa .

geant:location a nml:Location,
        owl:NamedIndividual ;
    nml:lat "48.859"^^xsd:float ;
    nml:long "2.295"^^xsd:float .

geant:nsa a nsi:NSA,
        owl:NamedIndividual ;
    nsi:adminContact """
            NSA: AutoBAHN  (GEANT)
            Project manager: 
            Radek Krzywania  
            email: radek.krzywania@man.poznan.pl  
            skype: Radoslaw.krzywania 
            Key software engineer: Michal Balcerkiewicz  
            email: michalb@man.poznan.pl  
            skype= michalb.psnc
        """^^xsd:string ;
    nsi:csProviderEndpoint """
           http://gnscdemo.geant.net:8080/nsi/ConnectionProvider
        """^^xsd:string ;
    nsi:manages geant:ets .

geant:geant-netherlight a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:geant-netherlight> ;
    nmleth:vlans "1780-1783" .

geant:geant-pionier a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:pionier.net.pl:2012:geant-pionier> ;
    nmleth:vlans "1780-1783" .

geant:netherlight-geant a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:netherlight.net:2012:netherlight-geant> ;
    nmleth:vlans "1780-1783" .

geant:pionier-geant a nml:PortGroup,
        owl:NamedIndividual ;
    nml:isAlias <urn:ogf:network:pionier.net.pl:2012:pionier-geant> ;
    nmleth:vlans "1780-1783" .

geant:ps-in a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

geant:ps-out a nml:PortGroup,
        owl:NamedIndividual ;
    nmleth:vlans "1780-1783" .

